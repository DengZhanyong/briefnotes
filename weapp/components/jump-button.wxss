.jump-button {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 14px 0;
  margin: 0 0 0 12px;
  border-bottom: 1px solid #eee;
  color: #333;
  font-size: 16px;
}
.jump-button .info {
  font-size: 12px;
  color: #999;
  margin-right: 3px;
}
.jump-button .icon {
  position: relative;
  top: 3px;
  height: 14px;
  width: 14px;
  margin-right: 8px;
}

